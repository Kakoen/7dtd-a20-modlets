<WrathmaniacsFoodWaterBar>
	<set xpath="/windows/window[@name='HUDLeftStatBars']/rect[@name='hud']/rect[@stat_type='Stamina' ]/@pos">0,0</set>
	<set xpath="/windows/window[@name='HUDLeftStatBars']/rect[@name='hud']/rect[@stat_type='Health' ]/@pos">0,50</set>
	<setattribute xpath="/windows/window[@name='HUDLeftStatBars']/rect[@name='hud']/rect[@stat_type='Stealth']" name="pos">0,100</setattribute>
	<set xpath="/windows/window[@name='HUDLeftStatBars']/rect[@side='left']/@pos">90,282</set>

	<append xpath="/windows/window[@name='HUDLeftStatBars']/rect[@side='right']">
			<rect pos="0,-50"  width="171"  height="46"  controller="HUDStatBar"  stat_type="Food"  visible="{statvisible}" >
				<sprite depth="1"  name="border"  color="0,0,0,128"  type="sliced" />
				<sprite depth="2"  name="background"  color="0,0,0,140"  type="sliced" />
				<filledsprite depth="3"  name="background"  color="110,110,110,128"  type="filled"  fill="{statmodifiedmax}"  width="169" />
				<filledsprite depth="5"  name="BarContent"  color="0,255,39,128"  type="filled"  fill="0"  width="169" />
				<sprite depth="6"  name="Icon"  atlas="{staticonatlas|once}"  sprite="ui_game_symbol_cookware"  size="35,35"  pos="8,-6"  foregroundlayer="true" />
				<label depth="7"  name="TextContent"  pos="6,-8"  font_size="32"  color="[white]"  justify="center"  pivot="topleft"  text="{statcurrentwithmax}"  height="35" />
				<sprite depth="8"  name="border"  color="0,0,0,255"  type="sliced"  fillcenter="false" />
				<filledsprite depth="8"  name="background"  color="0,0,0,255"  type="filled"  fill="{statmodifiedmax}"  width="171"  fillcenter="false" />
				<filledsprite depth="3"  name="background"  color="0,0,0,255"  type="filled"  fill="{statmodifiedmax}"  width="170"  fillcenter="false" />
			</rect>
			<rect pos="0,-100"  width="171"  height="46"  controller="HUDStatBar"  stat_type="Water"  visible="{statvisible}" >
				<sprite depth="1"  name="border"  color="0,0,0,128"  type="sliced" />
				<sprite depth="2"  name="background"  color="0,0,0,140"  type="sliced" />
				<filledsprite depth="3"  name="background"  color="110,110,110,128"  type="filled"  fill="{statmodifiedmax}"  width="169" />
				<filledsprite depth="5"  name="BarContent"  color="0,255,255,128"  type="filled"  fill="0"  width="169" />
				<sprite depth="6"  name="Icon"  atlas="{staticonatlas|once}"  sprite="ui_game_symbol_water"  size="35,35"  pos="8,-6"  foregroundlayer="true" />
				<label depth="7"  name="TextContent"  pos="6,-8"  font_size="32"  color="[white]"  justify="center"  pivot="topleft"  text="{statcurrentwithmax}"  height="35" />
				<sprite depth="8"  name="border"  color="0,0,0,255"  type="sliced"  fillcenter="false" />
				<filledsprite depth="8"  name="background"  color="0,0,0,255"  type="filled"  fill="{statmodifiedmax}"  width="171"  fillcenter="false" />
				<filledsprite depth="3"  name="background"  color="0,0,0,255"  type="filled"  fill="{statmodifiedmax}"  width="170"  fillcenter="false" />
			</rect>
	</append>
</WrathmaniacsFoodWaterBar>
